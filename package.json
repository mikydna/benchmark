{
  "name": "benchmark",
  "version": "0.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikydna/benchmark.git"
  },
  "scripts": {
    "build-lib": "rm -rf lib && jspm bundle-sfx --minify --skip-source-maps src/index lib/index.js",
    "build-bin": "rm -rf bin && jspm bundle-sfx --minify --skip-source-maps src/bin/cli bin/cli.js",
    "build": "npm run build-lib && npm run build-bin",
    "example": "jspm run src/cli",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "benchmark": "bin/cli"
  },
  "main": "lib/main.js",
  "author": "Andy K <mikydna@gmail.com>",
  "license": "ISC",
  "jspm": {
    "dependencies": {
      "babel-core": "npm:babel-core@^6.5.2",
      "babel-preset-es2015": "npm:babel-preset-es2015@^6.5.0",
      "babel-preset-stage-0": "npm:babel-preset-stage-0@^6.5.0",
      "chalk": "npm:chalk@^1.1.1",
      "fs-extra": "npm:fs-extra@^0.26.5",
      "lodash": "npm:lodash@^4.5.1",
      "minimist": "npm:minimist@^1.2.0",
      "module": "github:jspm/nodelibs-module@^0.1.0",
      "node-uuid": "npm:node-uuid@^1.4.7",
      "path": "github:jspm/nodelibs-path@^0.1.0",
      "rx": "npm:rx@^4.0.8",
      "vm": "github:jspm/nodelibs-vm@^0.1.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  },
  "devDependencies": {
    "jspm": "^0.16.29"
  }
}
